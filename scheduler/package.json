{
  "name": "trafExOpt",
  "version": "1.0.0",
  "description": "First of all get resource usage from Nodes, Pods and the available host machines of the cluster. After place a service, resource usage and availability of resources at the host are taken into account",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "ts-check": "tsc --noEmit",
    "build:clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf ./dist",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --testTimeout=30000",
    "test:ci": "yarn test --colors --coverage --ci",
    "test:stress": "k6 run --compatibility-mode=experimental_enhanced tests/k6/script.ts",
    "test:coverage": "yarn test --coverage",
    "prettier": "prettier --check --ignore-path .gitignore .",
    "prettier:fix": "prettier --write --ignore-path .gitignore .",
    "eslint": "eslint .",
    "lint": "yarn tsc --noEmit && yarn eslint && yarn prettier",
    "lint:fix": "yarn eslint --fix && yarn prettier:fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@kubernetes/client-node": "1.2.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.9.0",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "figlet": "^1.8.0",
    "js-yaml": "^4.1.0",
    "winston": "^3.17.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@jest/globals": "^29.7.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/k6": "^0.54.2",
    "@types/node": "22.10.1",
    "@types/request": "^2.48.12",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "resolutions": {
    "tough-cookie": "^4.1.3",
    "@babel/helpers": ">=7.26.10"
  }
}
